{
    "version": 2,
    "alias": "spotifycardio.com",
    "builds": [
      { "src": "/public/*", "use": "@vercel/public" },
      {
        "src": "next.config.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "dist/$1" }
    ],
    "env": {
      "SPOTIFY_CLIENT_ID": "@spotify_client_id",
      "SPOTIFY_CLIENT_SECRET": "@spotify_client_secret"
    }
  }
  